         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="LabelName" layoutX="351.0" layoutY="57.0" text="Label">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <ListView fx:id="ListPrice" layoutX="188.0" layoutY="112.0" prefHeight="200.0" prefWidth="200.0" />
      <Spinner fx:id="AmountCounter" layoutX="31.0" layoutY="200.0" />
      <TextField fx:id="FieldPriceAdd" layoutX="208.0" layoutY="200.0" onAction="#gettingPrice" />
      <Label layoutX="69.0" layoutY="183.0" text="Количество" />
      <Label fx:id="LabelAdd" layoutX="242.0" layoutY="183.0" text="Установите цену" />
      <Label fx:id="LabelChoice" layoutX="255.0" layoutY="95.0" text="Выберите цену" />
   </children>
</AnchorPane>
